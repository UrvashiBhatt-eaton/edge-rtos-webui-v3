{
  "name": "low-toolkit-web-ui-seed",
  "version": "2.14.0",
  "private": true,
  "homepage": "/if/",
  "scripts": {
    "analyze_note": "in .env set GENERATE_SOURCEMAP=true before running analyze",
    "analyze": "source-map-explorer build/static/js/*.js",
    "start_noport": "react-scripts start",
    "start": "set PORT=9001 && react-scripts start",
    "start_concurrently": "concurrently --kill-others 'set PORT=9001 && react-scripts start'",
    "build": "react-scripts build",
    "postbuild": "sh ./postBuildCleanup.sh",
    "gzip": "gzipper -v ./build ./prod-gzip",
    "link:mac": "sudo ./linkDependencies.sh",
    "link:win": "sh ./linkDependencies.sh",
    "relink:win": "sh ./linkDependencies.sh 1",
    "linkoursstart:win": "sh ./linkDependencies.sh 1 'oursOnly' && npm run-script start",
    "linkbluestart:win": "sh ./linkDependencies.sh 1 'blueOnly' && npm run-script start",
    "linkbluelayoutsstart:win": "sh ./linkDependencies.sh 1 'blueLayoutsOnly' && npm run-script start",
    "link_build_i18n:win": "set REACT_APP_LOCALIZE=true&&npm run-script link:win&&npm run-script build",
    "relink_build_i18n:win": "set REACT_APP_LOCALIZE=true&&npm run-script relink:win&&npm run-script build",
    "linkstart:win": "npm run-script relink:win && npm run-script start",
    "linkstart_i18n:win": "set REACT_APP_LOCALIZE=true&&npm run-script relink:win&&npm run-script start",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "CI=true react-scripts test --env=jsdom --forceExit --detectOpenHandles",
    "eject": "react-scripts eject",
    "test:open": "react-scripts test ./src/App.test.js --detectOpenHandles --env=jsdom",
    "getstrings": "node_modules\\.bin\\i18next 'src\\**\\*.{js,jsx,ts}' 'PxBlueModule\\layouts\\dist\\**\\*.{js,jsx,ts}' 'PxBlueModule\\seed-router\\dist\\**\\*.{js,jsx,ts}' 'PxBlueModule\\utilities\\dist\\**\\*.{js,jsx,ts}' 'PxGreenUIModule\\dist\\**\\*.{js,jsx,ts}'"
  },
  "dependencies": {
    "@brightlayer-ui/colors": "^3.1.0",
    "@brightlayer-ui/icons-mui": "^3.0.0",
    "@brightlayer-ui/react-components": "^6.0.0",
    "@brightlayer-ui/react-themes": "^7.0.0",
    "@brightlayer-ui/PxBlueModule": "git@github.com:etn-ccis/ux-seed-modules.git#3d6f5c5c7be9e93bf58291768c660d2b6467a907",
    "@date-io/date-fns": "^1.3.13",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/lab": "^5.0.0-alpha.84",
    "@mui/material": "^5.8.2",
    "@mui/styles": "^5.8.6",
    "@mui/x-date-pickers": "^5.0.0-alpha.5",
    "@types/react-redux": "^7.1.11",
    "date-fns": "^2.14.0",
    "html-react-parser": "^1.2.8",
    "i18next": "^19.9.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-http-backend": "^1.2.6",
    "jss-rtl": "^0.3.0",
    "md5": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.16.9",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "stylis": "^4.1.1",
    "stylis-plugin-rtl": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^7.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gzipper": "^3.7.0",
    "i18next-parser": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "javascript-obfuscator": "^3.0.0",
    "react-scripts": "^5.0.1",
    "seamless-immutable": "^7.1.3",
    "source-map-explorer": "^1.7.0",
    "typescript": "^4.7.4"
  },
  "browserslist": [
    "defaults"
  ]
}
